    <!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985)             -->
    <input spec='HKY' id="hky">
        <parameter name='kappa' idref='hky.kappa'/>
        <input id='freqs' name='frequencies' spec='Frequencies' frequencies='@hky.frequencies'/>
    </input>

    <!-- site model                                                              -->
    <input spec='SiteModel' id="siteModel">
        <!--<parameter name='mutationRate'>1.0</parameter>-->
        <substModel idref='hky'/>
    </input>

    <input spec='TreeLikelihood' id="treeLikelihood">
        <data idref="alignment"/>
        <tree idref="tree"/>
        <siteModel idref="siteModel"/>
    </input>


    <input spec='beast.base.spec.evolution.tree.ClusterTree' id='tree' clusterType='upgma'>
        <taxa idref='alignment'/>
    </input>

    <!-- Parameters -->
    <parameter id="hky.kappa" value="2.0" spec="RealScalarParam" domain="PositiveReal"/>
    <parameter id="hky.frequencies" dimension="4" value="0.25" spec="SimplexParam"/>
    <parameter id="popSize" value="0.077" spec="RealScalarParam" domain="PositiveReal"/>
    <parameter id="growthRate" value="7.7E-5" spec="RealScalarParam" domain="Real"/>

    <run spec="MCMC" id="mcmc" chainLength="10000000"> <!--autoOptimize="true"-->
        <state>
            <!--
                        <tree spec='util.TreeParser' id='tree'>
                            <input name='newick'>((5:0.04847887618847128,(3:0.03189934232093919,((2:0.011076861832266626,1:0.011076861832266626):0.009810542752873795,4:0.02088740458514042):0.011011937735798769):0.01657953386753209):0.017232517763959114,6:0.06571139395243039);</input>
                           <input name='taxa' idref='alignment'/>
                        </tree>
            -->
            <input name='stateNode' idref='hky.kappa'/>
            <input name='stateNode' idref='hky.frequencies'/>
            <input name='stateNode' idref='popSize'/>
            <input name='stateNode' idref='growthRate'/>
            <input name='stateNode' idref='tree'/>
        </state>

        <distribution spec="CompoundDistribution" id="posterior">
            <distribution spec="CompoundDistribution" id="prior">
                <!-- kappa prior -->
                <distribution id="hky.kappa.prior" spec="LogNormal" param="@hky.kappa">
                    <M spec="RealScalarParam" domain="Real" value="1.0"/>
                    <S spec="RealScalarParam" domain="PositiveReal" value="1.25"/>
                </distribution>

                <!-- frequencies prior -->
                <distribution id="hky.frequencies.prior" spec="beast.base.spec.inference.distribution.IID" param="@hky.frequencies">
                    <distr spec="beast.base.spec.inference.distribution.Uniform">
                        <lower spec="RealScalarParam" domain="Real" value="0.0"/>
                        <upper spec="RealScalarParam" domain="Real" value="1.0"/>
                    </distr>
                </distribution>

                <!-- popSize prior -->
                <distribution id="popSize.prior" spec="beast.base.spec.beast2vs1.OneOnX" param="@popSize"/>

                <!-- growthRate prior -->
                <distribution id="growthRate.prior" spec="beast.base.spec.inference.distribution.Laplace"
                              param="@growthRate">
                    <mu spec="RealScalarParam" domain="Real" value="0.001"/>
                    <scale spec="RealScalarParam" domain="PositiveReal" value="119.61481002566472"/>
                </distribution>

                <!-- Coalescent -->
                <distribution id="coalescent" spec="Coalescent">
                    <treeIntervals spec="beast.base.evolution.tree.TreeIntervals">
                        <tree idref="tree"/>
                    </treeIntervals>
                    <populationModel spec="beast.base.spec.evolution.tree.coalescent.ExponentialGrowth">
                        <popSize idref="popSize"/>
                        <growthRate idref="growthRate"/>
                    </populationModel>
                </distribution>

            </distribution>

            <distribution id='likelihood' idref="treeLikelihood"/>
        </distribution>

        <operator id='kappaScaler' spec='beast.base.spec.inference.operator.ScaleOperator'
                  scaleFactor="0.75" weight="0.1" parameter="@hky.kappa"/>
        <operator id='frequenciesDelta' spec='beast.base.spec.inference.operator.DeltaExchangeOperator'
                  delta="0.01" weight="0.1" rvparameter="@hky.frequencies"/>
        <operator id='popSizeScaler' spec='beast.base.spec.inference.operator.ScaleOperator'
                  scaleFactor="0.75" weight="3" parameter="@popSize"/>
        <operator id='growthRateRandomWalk' spec='beast.base.spec.inference.operator.RealRandomWalkOperator'
                  windowSize="1.0" weight="3" scalar="@growthRate"/>
        <operator id='treeScaler' spec='beast.base.spec.evolution.operator.ScaleTreeOperator'
                  scaleFactor="0.75" weight="3" tree="@tree"/>

        <operator spec='Uniform' weight="30" tree="@tree"/>
        <operator spec='SubtreeSlide' weight="15" gaussian="true" size="0.0077" tree="@tree"/>
        <operator id='narrow' spec='Exchange' isNarrow='true' weight="15" tree="@tree"/>
        <operator id='wide' spec='Exchange' isNarrow='false' weight="3" tree="@tree"/>
        <operator spec='WilsonBalding' weight="1" tree="@tree"/>
        <operator spec='beast.base.spec.evolution.operator.UpDownOperator' scaleFactor="0.75" weight="3" down="@tree"/>

        <logger logEvery="1000" fileName="test.$(seed).log">
            <model idref='posterior'/>
            <log idref="posterior"/>
            <log idref="prior"/>
            <log spec='beast.base.evolution.tree.TreeStatLogger' tree='@tree'/>
            <log idref="popSize"/>
            <log idref="growthRate"/>
            <log idref="hky.kappa"/>
            <log idref="hky.frequencies"/>
            <log idref="likelihood"/>
            <log idref="coalescent"/>
        </logger>
        <!--        <logger logEvery="1000" fileName="test.$(seed).trees">
 <log idref="tree"/>
</logger>           -->
        <logger logEvery="100000">
            <model idref='posterior'/>
            <log idref="posterior"/>
            <log idref="prior"/>
            <log spec='beast.base.evolution.tree.TreeStatLogger' tree='@tree'/>
            <log idref="popSize"/>
            <log idref="growthRate"/>
            <log idref="hky.kappa"/>
            <log idref="hky.frequencies"/>
            <log idref="likelihood"/>
            <log idref="coalescent"/>
        </logger>
    </run>

</beast>
