    <!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985) -->
    <input spec='HKY' id="hky">
        <kappa idref='hky.kappa'/>
        <input id='freqs' name='frequencies' spec='Frequencies' frequencies='@hky.frequencies' />
    </input>

    <!-- site model -->
    <input spec='SiteModel' id="siteModel">
        <!--<parameter name='mutationRate'>1.0</parameter> -->
        <substModel idref='hky' />
    </input>


    <input spec='beast.base.spec.evolution.branchratemodel.UCRelaxedClockModel' id="branchRates">
        <!-- <parameter name='mutationRate'>1.0</parameter> -->
        <distr id='lognormal' spec="beast.base.spec.inference.distribution.LogNormal" meanInRealSpace="true">
            <M id='M' spec="RealScalarParam" domain="Real" value="1.0"/>
            <S id='S' spec="RealScalarParam" domain="PositiveReal" value="0.3333333333333333"/>
        </distr>
        <rateCategories spec='IntVectorParam' domain="NonNegativeInt" id='rateCategories' dimension="11" value="1" />
        <tree idref="tree" />
    </input> 

    <input spec='TreeLikelihood' id="treeLikelihood">
        <data idref="alignment" />
        <tree idref="tree" />
        <siteModel idref="siteModel" />
        <branchRateModel idref="branchRates" /> 
    </input>

    <tree spec='beast.base.evolution.tree.TreeParser' id='tree' IsLabelledNewick='true' newick=
            '(siamang:0.06318470579939774,((gorilla:0.031056173315146828,((chimp:0.00982869388112981,bonobo:0.00982869388112981)
        :0.01030402958978773,human:0.02013272347091754):0.010923449844229289):0.022256750048834654,orangutan:0.05331292336398148)
        :0.009871782435416254);'
          taxa='@alignment'/>

    <parameter id="hky.kappa" value="2.0" spec="RealScalarParam" domain="PositiveReal"/>
    <parameter dimension="4" id="hky.frequencies" spec="SimplexParam" value="0.25"/>
    <parameter id="popSize" value="0.077" spec="RealScalarParam" domain="PositiveReal"/>

    <run spec="MCMC" id="mcmc" chainLength="10000000"> <!--autoOptimize="true" -->

        <distribution spec="CompoundDistribution" id="posterior">
            <distribution spec="CompoundDistribution" id="prior">
                <distribution id="hky.kappa.prior" spec='LogNormal' param='@hky.kappa' >
                    <!-- M="1" S="1.25" />-->
                    <M spec="RealScalarParam" domain="Real" value="1.0"/>
                    <S spec="RealScalarParam" domain="PositiveReal" value="1.25"/>
                </distribution>
                <distribution id="hky.frequencies.prior" spec="beast.base.spec.inference.distribution.IID"
                              param="@hky.frequencies">
                    <distr id="Uniform" spec="beast.base.spec.inference.distribution.Uniform">
                        <lower id="freqs.lower" spec="RealScalarParam" domain="Real" value="0.0"/>
                        <upper id="freqs.upper" spec="RealScalarParam" domain="Real" value="1.0"/>
                    </distr>
                </distribution>
                <!-- OneOnX is deprecated, test only -->
                <distribution id="popSize.prior" spec='beast.base.spec.beast2vs1.OneOnX'
                              param='@popSize' />
                <distribution id="S.prior" spec="beast.base.spec.inference.distribution.Exponential"
                              param="@S">
                    <mean spec="RealScalarParam" domain="PositiveReal" value="0.3333333333333333"/>
                </distribution>   

                <distribution id="coalescent" spec="Coalescent">
                    <treeIntervals spec='beast.base.evolution.tree.TreeIntervals' id='TreeIntervals'>
                        <tree idref="tree" />
                    </treeIntervals>
                    <populationModel spec="beast.base.spec.evolution.tree.coalescent.ConstantPopulation" id='ConstantPopulation'>
                        <popSize idref="popSize"/>
                    </populationModel>
                </distribution>
            </distribution>
            <distribution id='likelihood' idref="treeLikelihood" />
        </distribution>

        <operator id='SScaler' spec='beast.base.spec.inference.operator.ScaleOperator'
                  parameter="@S" scaleFactor="0.75" weight="3"/>
        <operator id="CategoriesRandomWalk" spec="beast.base.spec.inference.operator.IntRandomWalkOperator"
                  parameter="@rateCategories" windowSize='1' weight="10" />
        <operator id='CategoriesSwapOperator' spec='beast.base.spec.inference.operator.SwapOperator'
                  intparameter='@rateCategories' howMany="1" weight="10" />
        <operator id='CategoriesUniform' spec='beast.base.spec.inference.operator.uniform.IntUniformOperator'
                  parameter='@rateCategories' weight="10"/>

        <operator id='kappaScaler' spec='beast.base.spec.inference.operator.ScaleOperator'
                  scaleFactor="0.75" weight="0.1" parameter="@hky.kappa"/>
        <operator id='frequenciesDelta' spec='beast.base.spec.inference.operator.DeltaExchangeOperator'
                  delta="0.01" weight="0.1" rvparameter="@hky.frequencies"/>
        <operator id='popSizeScaler' spec='beast.base.spec.inference.operator.ScaleOperator'
                  scaleFactor="0.75" weight="3" parameter="@popSize"/>
        <operator id='treeScaler' spec='beast.base.spec.evolution.operator.ScaleTreeOperator'
                  scaleFactor="0.75" weight="3" tree="@tree"/>
        <operator spec='Uniform' weight="30" tree="@tree" />
        <operator spec='SubtreeSlide' weight="15" gaussian="true" size="0.0077" tree="@tree" />
        <operator id='narrow' spec='Exchange' isNarrow='true' weight="15" tree="@tree" />
        <operator id='wide' spec='Exchange' isNarrow='false' weight="3" tree="@tree" />
        <operator spec='WilsonBalding' weight="1" tree="@tree" />
        <operator spec='beast.base.spec.evolution.operator.UpDownOperator' scaleFactor="0.75" weight="3" down="@tree" />

        <logger logEvery="10000" fileName="test.$(seed).log">
            <model idref='posterior' />
            <log idref="posterior" />
            <log idref="prior" />
            <log idref="treeLikelihood" />
            <log idref="coalescent" />
            <log idref="popSize" />
            <log spec='beast.base.evolution.tree.TreeStatLogger' tree='@tree' />
            <log idref="hky.kappa" />
            <log idref="hky.frequencies" />
            <log idref="M" />
            <log idref="S" />
            <log id='rate' spec='beast.base.evolution.RateStatistic' tree='@tree' branchratemodel='@branchRates'/>
            <log idref="rateCategories" /> 
        </logger>
        <!-- <logger logEvery="10000" fileName="test.$(seed).trees">
            <log idref="tree" />
        </logger> -->
        <logger logEvery="100000">
            <model idref='posterior' />
            <log idref="posterior" />
            <log idref="coalescent" />
            <log idref="treeLikelihood" />
            <log idref="popSize" />
            <log spec='beast.base.evolution.tree.TreeStatLogger' tree='@tree' />
            <log idref="hky.kappa" />
            <log idref="S" />
            <log idref="rateCategories" /> 
        </logger>
    </run>

</beast>
