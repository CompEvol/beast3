<beast version='2.0'
       namespace='beast.pkgmgmt:beast.base.core:beast.base.inference:
       beast.base.evolution.tree.coalescent:beast.evolution.nuc:beast.base.evolution.alignment:
       beast.base.evolution.operator:beast.base.spec.inference.operator:
       beast.base.spec.evolution.sitemodel:beast.base.spec.evolution.substitutionmodel:
       beast.base.spec.inference.parameter:beast.base.spec.inference.util:
       beast.base.spec.inference.distribution:beast.base.spec.evolution.likelihood'>

	<data dataType="nucleotide" id="alignment">
		<sequence taxon="D4Brazi82">
			
		</sequence>
		<sequence taxon="D4ElSal83">
			
		</sequence>
		<sequence taxon="D4ElSal94">
			
		</sequence>
		<sequence taxon="D4Indon76">
			
		</sequence>
		<sequence taxon="D4Indon77">
			
		</sequence>
		<sequence taxon="D4Mexico84">
			
		</sequence>
		<sequence taxon="D4NewCal81">
			
		</sequence>
		<sequence taxon="D4Philip64">
			
		</sequence>
		<sequence taxon="D4Philip56">
			
		</sequence>
		<sequence taxon="D4Philip84">
			
		</sequence>
		<sequence taxon="D4PRico86">
			
		</sequence>
		<sequence taxon="D4SLanka78">
			
		</sequence>
		<sequence taxon="D4Tahiti79">
			
		</sequence>
		<sequence taxon="D4Tahiti85">
			
		</sequence>
		<sequence taxon="D4Thai63">
			
		</sequence>
		<sequence taxon="D4Thai78">
			
		</sequence>
		<sequence taxon="D4Thai84">
			
		</sequence>
	</data>

	<!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985) -->
	<input spec='HKY' id="hky">
		<parameter name='kappa' idref='hky.kappa' />
		<input id='freqs' name='frequencies' spec='Frequencies' frequencies='@hky.frequencies' />
	</input>

	<!-- site model -->
	<input spec='SiteModel' id="siteModel">
		<!--<parameter name='mutationRate'>1.0</parameter> -->
		<substModel idref='hky' />
	</input>
	
	<input spec='beast.base.spec.evolution.branchratemodel.StrictClockModel' id="branchRates">
		<clock.rate idref='clockRate' />
	</input>

	<input spec='TreeLikelihood' id="treeLikelihood">
		<data idref="alignment" />
		<tree idref="tree" />
		<siteModel idref="siteModel" />
		<branchRateModel idref="branchRates" />
	</input>

	<input spec='beast.base.spec.evolution.tree.ClusterTree' id='tree' clusterType='upgma'>
		<trait spec='beast.base.evolution.tree.TraitSet' traitname='date-forward' units='year'
			value='
        D4Brazi82  = 1982, D4ElSal83  = 1983, D4ElSal94  = 1994, D4Indon76  = 1976, D4Indon77  = 1977,
        D4Mexico84  = 1984, D4NewCal81  = 1981, D4Philip64  = 1964, D4Philip56  = 1956, D4Philip84  = 1984,
        D4PRico86  = 1986, D4SLanka78  = 1978, D4Tahiti79  = 1979, D4Tahiti85  = 1985, D4Thai63  = 1963,
        D4Thai78  = 1978, D4Thai84 = 1984'>
			<taxa spec='beast.base.evolution.alignment.TaxonSet' alignment='@alignment' />
		</trait>
		<taxa idref='alignment' />
	</input>


	<parameter id='clockRate' value="2.3E-5" spec="RealScalarParam" domain="PositiveReal" />
    <parameter id="hky.kappa" value="2.0" spec="RealScalarParam" domain="PositiveReal"/>
    <parameter dimension="4" id="hky.frequencies" spec="SimplexParam" value="0.25"/>
    <parameter id="popSize" value="380.0" spec="RealScalarParam" domain="PositiveReal"/>

	<run spec="MCMC" id="mcmc" chainLength="10000000"> <!--autoOptimize="true" -->
		<state>
			<stateNode idref='hky.kappa' />
			<stateNode idref='popSize' />
			<stateNode idref='hky.frequencies' />
			<stateNode idref='tree' />
			<stateNode idref='clockRate' />
		</state>

		<distribution spec="CompoundDistribution" id="posterior">
			<distribution spec="CompoundDistribution" id="prior">
                <distribution id="hky.kappa.prior" spec='LogNormal' param='@hky.kappa' >
                    <!-- M="1" S="1.25" />-->
                    <M spec="RealScalarParam" domain="Real" value="1.0"/>
                    <S spec="RealScalarParam" domain="PositiveReal" value="1.25"/>
                </distribution>
                <distribution id="hky.frequencies.prior" spec="beast.base.spec.inference.distribution.IID"
                              param="@hky.frequencies">
                    <distr id="Uniform" spec="beast.base.spec.inference.distribution.Uniform">
                        <lower id="freqs.lower" spec="RealScalarParam" domain="Real" value="0.0"/>
                        <upper id="freqs.upper" spec="RealScalarParam" domain="Real" value="1.0"/>
                    </distr>
                </distribution>
                <!-- OneOnX is deprecated, test only -->
                <distribution id="popSize.prior" spec='beast.base.spec.beast2vs1.OneOnX'
                              param='@popSize' />

				<distribution id="clockRate.prior" spec="beast.base.spec.inference.distribution.Uniform"
                              param="@clockRate">
                    <lower id="clockRate.lower" spec="RealScalarParam" domain="Real" value="0.0"/>
                    <upper id="clockRate.upper" spec="RealScalarParam" domain="Real" value="100.0"/>
				</distribution>

				<distribution id="coalescent" spec="Coalescent">
					<treeIntervals spec='beast.base.evolution.tree.TreeIntervals' id='TreeIntervals'>
						<tree idref="tree" />
					</treeIntervals>
					<populationModel spec="beast.base.spec.evolution.tree.coalescent.ConstantPopulation">
						<popSize idref="popSize" />
					</populationModel>
				</distribution>
			</distribution>
			<distribution id='likelihood' idref="treeLikelihood" />
		</distribution>

        <operator id='kappaScaler' spec='beast.base.spec.inference.operator.ScaleOperator'
                  scaleFactor="0.75" weight="0.1" parameter="@hky.kappa"/>
        <operator id='frequenciesDelta' spec='beast.base.spec.inference.operator.DeltaExchangeOperator'
                  delta="0.01" weight="0.1" rvparameter="@hky.frequencies"/>
        <operator id='popSizeScaler' spec='beast.base.spec.inference.operator.ScaleOperator'
                  scaleFactor="0.75" weight="3" parameter="@popSize"/>
        <operator id='treeScaler' spec='beast.base.spec.evolution.operator.ScaleTreeOperator'
                  scaleFactor="0.75" weight="3" tree="@tree"/>
        <operator spec='Uniform' weight="30" tree="@tree"/>
        <operator spec='SubtreeSlide' weight="15" gaussian="true" size="0.0077" tree="@tree" />
		<operator id='narrow' spec='Exchange' isNarrow='true' weight="15" tree="@tree" />
		<operator id='wide' spec='Exchange' isNarrow='false' weight="3" tree="@tree" />
		<operator spec='WilsonBalding' weight="3" tree="@tree" />
		<operator spec='beast.base.spec.evolution.operator.UpDownOperator' scaleFactor="0.75" weight="3" up="@clockRate" down="@tree" />
		<operator id='clockRateScaler' spec='beast.base.spec.inference.operator.ScaleOperator'
                  scaleFactor="0.75" weight="3" parameter="@clockRate" />

		<logger logEvery="1000" fileName="test.$(seed).log">
			<model idref='posterior' />
			<log idref="posterior" />
			<log idref="prior" />
			<log spec='beast.base.evolution.tree.TreeStatLogger' tree='@tree' />
			<log idref="popSize" />
			<log idref="hky.kappa" />
			<log idref="hky.frequencies" />
			<log idref="clockRate" />
			<log idref="likelihood" />
			<log idref="coalescent" />
		</logger>
		<!-- <logger logEvery="1000" fileName="test.$(seed).trees"> <log idref="tree"/> </logger> -->
		<logger logEvery="100000">
			<model idref='posterior' />
			<log idref="posterior" />
			<log idref="prior" />
			<log spec='beast.base.evolution.tree.TreeStatLogger' tree='@tree' />
			<log idref="popSize" />
			<log idref="hky.kappa" />
			<log idref="hky.frequencies" />
			<log idref="clockRate" />
			<log idref="likelihood" />
			<log idref="coalescent" />
		</logger>
	</run>

</beast>
