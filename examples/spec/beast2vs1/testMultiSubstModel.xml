<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beast version='2.0'
       namespace='beast.pkgmgmt:beast.base.core:beast.base.inference:
       beast.base.evolution.tree.coalescent:beast.evolution.nuc:beast.base.evolution.alignment:
       beast.base.evolution.operator:beast.base.spec.inference.operator:
       beast.base.spec.evolution.sitemodel:beast.base.spec.evolution.substitutionmodel:
       beast.base.spec.inference.parameter:beast.base.spec.inference.util:
       beast.base.spec.inference.distribution:beast.base.spec.evolution.likelihood'>


    <data dataType="nucleotide" filter="1-449" id="firsthalf" name="alignment" spec="FilteredAlignment" strip="false">
                    <data idref="Primates"/>
                </data>
<data dataType="nucleotide" id="Primates" strip="false">
                        <sequence id="seq_Tarsius_syrichta" taxon="Tarsius_syrichta" totalcount="4" value="-TGCAAGAACTGCTAACTCATGCCTCCATATATAACAATGTGGCTTTCTT-ACTTTTAAAGGATAGAAGTAATCCATCGGTCTTAGGAACCGAAAA-ATTGGTGCAACTCCAAATAAAAGTAATAAATTTATTTTCATCCTCCATTTTACTATCACTTACACTCTTAATTACCCCATTTATTATTACAACAACTAAAAAATATGAAACACATGCATACCCTTACTACGTAAAAAACTCTATCGCCTGCGCATTTATAACAAGCCTAGTCCCAATGCTCATATTTCTATACACAAATCAAGAAATAATCATTTCCAACTGACATTGAATAACGATTCATACTATCAAATTATGCCTAAGCTT"/>
                        <sequence id="seq_Lemur_catta" taxon="Lemur_catta" totalcount="4" value="-AAAACACTAGATTGTGAATCCAGAAATAGAAGCTCAAAC-CTTCTTATTTACCGAGAAAGTAATGTATGAACTGCTAACTCTGCACTCCGTATATAAAAATACGGCTATCTCAACTTTTAAAGGATAGAAGTAATCCATTGGCCTTAGGAGCCAAAAA-ATTGGTGCAACTCCAAATAAAAGTAATAAATCTATTATCCTCTTTCACCCTTGTCACACTGATTATCCTAACTTTACCTATCATTATAAACGTTACAAACATATACAAAAACTACCCCTATGCACCATACGTAAAATCTTCTATTGCATGTGCCTTCATCACTAGCCTCATCCCAACTATATTATTTATCTCCTCAGGACAAGAAACAATCATTTCCAACTGACATTGAATAACAATCCAAACCCTAAAACTATCTATTAGCTT"/>
                        <sequence id="seq_Homo_sapiens" taxon="Homo_sapiens" totalcount="4" value="-CGACCCCTTATTTACCGAGAAAGCT-"/>
                        <sequence id="seq_Pan" taxon="Pan" totalcount="4" value="-CGACCCCTTATTTACCGAGAAAGCT-"/>
                        <sequence id="seq_Gorilla" taxon="Gorilla" totalcount="4" value="-CAACCCCTTATTTACCGAGAAAGCT-"/>
                        <sequence id="seq_Pongo" taxon="Pongo" totalcount="4" value="-CAACCCCTTATTTACCGAGAAAGCT-CACAAGAACTGCTAACTCTCACT-"/>
                        <sequence id="seq_Hylobates" taxon="Hylobates" totalcount="4" value="-AAACCTCTTGCTTACCGAGAAAGCC-"/>
                        <sequence id="seq_Macaca_fuscata" taxon="Macaca_fuscata" totalcount="4" value="-CCACCTCTTATTTACCGAGAAAACT-"/>
                        <sequence id="seq_M_mulatta" taxon="M_mulatta" totalcount="4" value="-CCACCTCTTATTTACCGAGAAAACT-"/>
                        <sequence id="seq_M_fascicularis" taxon="M_fascicularis" totalcount="4" value="-CCACTTCTTATTTACCGAGAAAACT-"/>
                        <sequence id="seq_M_sylvanus" taxon="M_sylvanus" totalcount="4" value="-CCACTTCTTATTTACCGAGAAAACT-"/>
                        <sequence id="seq_Saimiri_sciureus" taxon="Saimiri_sciureus" totalcount="4" value="-TAACTTCTTAATTACCGAGAAAGTG-CGCAAGAACTGCTAATTCATGCTCCCAAGACTAACAACTTGGCTTCCTCAACTTTTAAAGGATAGTAGTTATCCATTGGTCTTAGGAGCCAAAAACATTGGTGCAACTCCAAATAAAAGTAATA---ATACACTTCTCCATCACTCTAATAACACTAATTAGCCTACTAGCGCCAATCCTAGCTACCCTCATTAACCCTAACAAAAGCACACTATACCCGTACTACGTAAAACTAGCCATCATCTACGCCCTCATTACCAGTACCTTATCTATAATATTCTTTATCCTTACAGGCCAAGAATCAATAATTTCAAACTGACACTGAATAACTATCCAAACCATCAAACTATCCCTAAGCTT"/>
                    </data>
<data data="@Primates" dataType="nucleotide" filter="450-898" id="secondhalf" spec="FilteredAlignment" strip="false"/>


    <distribution id="posterior" spec="CompoundDistribution" useThreads="false">
        <distribution id="prior" spec="CompoundDistribution" useThreads="false">
            <distribution id="CoalescentConstant.firsthalf" spec="Coalescent">
                <populationModel id="ConstantPopulation.firsthalf" spec="beast.base.spec.evolution.tree.coalescent.ConstantPopulation">
                    <popSize id="popSize.firsthalf" spec="RealScalarParam" domain="PositiveReal" value="1.0" estimate="true"/>
                </populationModel>
                <treeIntervals id="TreeIntervals.firsthalf" spec="beast.base.evolution.tree.TreeIntervals" tree="@Tree.firsthalf"/>
            </distribution>
            <distribution id="KappaPrior.firsthalf" spec='LogNormal' param='@kappa.firsthalf' >
                <!-- M="1" S="1.25" />-->
                <M spec="RealScalarParam" domain="Real" value="1.0"/>
                <S spec="RealScalarParam" domain="PositiveReal" value="1.25"/>
            </distribution>
            <distribution id="KappaPrior.secondhalf" spec='LogNormal' param='@kappa.secondhalf' >
                <M spec="RealScalarParam" domain="Real" value="1.0"/>
                <S spec="RealScalarParam" domain="PositiveReal" value="1.25"/>
            </distribution>
<!-- OneOnX is deprecated, test only -->
            <distribution id="popSize.prior" spec='beast.base.spec.beast2vs1.OneOnX'
                          param='@popSize.firsthalf' />
        </distribution>
        <distribution id="likelihood" spec="CompoundDistribution" useThreads="false">
            <distribution data="@firsthalf" id="treeLikelihood.firsthalf" spec="TreeLikelihood" tree="@Tree.firsthalf" useAmbiguities="false">
                <siteModel gammaCategoryCount="0" id="SiteModel.firsthalf" spec="SiteModel">
                    <parameter estimate="false" id="mutationRate.firsthalf" name="mutationRate" spec="RealScalarParam" domain="PositiveReal" value="1.0"/>
                    <parameter estimate="false" id="gammaShape.firsthalf" name="shape" spec="RealScalarParam" domain="PositiveReal" value="1.0"/>
                    <parameter estimate="false" id="proportionInvariant.firsthalf" name="proportionInvariant" spec="RealScalarParam" domain="UnitInterval" value="0.0"/>
                    <substModel id="hky.firsthalf" kappa="@kappa.firsthalf" spec="HKY">
                        <frequencies estimate="true" frequencies="@freqParameter.firsthalf" id="estimatedFreqs.firsthalf" spec="Frequencies"/>
                    </substModel>
                </siteModel>
                <branchRateModel clock.rate="@clockRate.firsthalf" id="StrictClock.firsthalf" spec="beast.base.spec.evolution.branchratemodel.StrictClockModel"/>
            </distribution>
            <distribution branchRateModel="@StrictClock.firsthalf" id="treeLikelihood.secondhalf" spec="TreeLikelihood" tree="@Tree.firsthalf" useAmbiguities="false">
                <data idref="secondhalf"/>
                <siteModel gammaCategoryCount="0" id="SiteModel.secondhalf" spec="SiteModel">
                    <parameter estimate="false" id="mutationRate.secondhalf" name="mutationRate" spec="RealScalarParam" domain="PositiveReal" value="1.0"/>
                    <parameter estimate="false" id="gammaShape.secondhalf" name="shape" spec="RealScalarParam" domain="PositiveReal" value="1.0"/>
                    <parameter estimate="false" id="proportionInvariant.secondhalf" name="proportionInvariant" spec="RealScalarParam" domain="UnitInterval" value="0.0"/>
                    <substModel id="hky.secondhalf" kappa="@kappa.secondhalf" spec="HKY">
                        <frequencies estimate="true" frequencies="@freqParameter.secondhalf" id="estimatedFreqs.secondhalf" spec="Frequencies"/>
                    </substModel>
                </siteModel>
            </distribution>
        </distribution>
    </distribution>

<parameter id="clockRate.firsthalf" spec="RealScalarParam" domain="PositiveReal" value="1.0"/>
    
<run chainLength="10000000" id="mcmc" preBurnin="0" sampleFromPrior="false" spec="MCMC" storeEvery="-1">
    <state id="state" storeEvery="100000">
        <tree estimate="true" id="Tree.firsthalf" name="stateNode">
            <taxonset id="TaxonSet.firsthalf" spec="TaxonSet">
                <data idref="firsthalf" name="alignment"/>
            </taxonset>
        </tree>
        <stateNode idref='popSize.firsthalf'/>
        <parameter estimate="true" id="kappa.firsthalf" spec="RealScalarParam" domain="PositiveReal" name="stateNode" value="1.0"/>
        <parameter dimension="4" estimate="true" id="freqParameter.firsthalf" spec="SimplexParam" name="stateNode" value="0.25"/>
        <parameter estimate="true" id="kappa.secondhalf" spec="RealScalarParam" domain="PositiveReal" name="stateNode" value="1.0"/>
        <parameter dimension="4" estimate="true" id="freqParameter.secondhalf" spec="SimplexParam" name="stateNode" value="0.25"/>
    </state>

    <distribution idref="posterior"/>

    <operator degreesOfFreedom="1" id="treeScaler.firsthalf" optimise="true" rootOnly="false"
              scaleAll="false" scaleAllIndependently="false" scaleFactor="0.75"
              spec="ScaleOperator" tree="@Tree.firsthalf" weight="3.0"/>

    <operator id="UniformOperator.firsthalf" spec="Uniform" tree="@Tree.firsthalf" weight="30.0"/>

    <operator gaussian="true" id="SubtreeSlide.firsthalf" optimise="true" size="0.03" spec="SubtreeSlide" tree="@Tree.firsthalf" weight="15.0"/>

    <operator id="narrow.firsthalf" isNarrow="true" spec="Exchange" tree="@Tree.firsthalf" weight="15.0"/>

    <operator id="wide.firsthalf" isNarrow="false" spec="Exchange" tree="@Tree.firsthalf" weight="3.0"/>

    <operator id="WilsonBalding.firsthalf" spec="WilsonBalding" tree="@Tree.firsthalf" weight="3.0"/>

    <operator spec='beast.base.spec.evolution.operator.UpDownOperator' scaleFactor="0.75" weight="3" down="@Tree.firsthalf"/>

    <operator id="KappaScaler.firsthalf" parameter="@kappa.firsthalf" scaleFactor="0.75"
              spec="beast.base.spec.inference.operator.ScaleOperator" weight="1.0"/>

    <operator autoOptimize="true" delta="0.01" id="FrequenciesExchanger.firsthalf"
              integer="false" spec="beast.base.spec.inference.operator.DeltaExchangeOperator" weight="0.1">
        <parameter idref="freqParameter.firsthalf"/>
    </operator>

    <operator id="KappaScaler.secondhalf" parameter="@kappa.secondhalf" scaleFactor="0.75"
              spec="beast.base.spec.inference.operator.ScaleOperator" weight="1.0"/>

    <operator autoOptimize="true" delta="0.01" id="FrequenciesExchanger.secondhalf"
              integer="false" spec="beast.base.spec.inference.operator.DeltaExchangeOperator" weight="0.1">
        <parameter idref="freqParameter.secondhalf"/>
    </operator>

    <operator id='popSizeScaler' spec='beast.base.spec.inference.operator.ScaleOperator' scaleFactor="0.75" weight="3" parameter="@popSize.firsthalf"/>

    <logger fileName="test.$(seed).log" id="tracelog" logEvery="1000" mode="autodetect" model="@posterior">
        <distribution idref="posterior" name="log"/>
        <distribution idref="likelihood" name="log"/>
        <distribution idref="prior" name="log"/>
        <log id="TreeHeight.firsthalf" spec="beast.base.evolution.tree.TreeStatLogger" tree="@Tree.firsthalf"/>
        <parameter idref="clockRate.firsthalf" name="log"/>
        <parameter idref="kappa.firsthalf" name="log"/>
        <parameter idref="freqParameter.firsthalf" name="log"/>
        <parameter idref="kappa.secondhalf" name="log"/>
        <parameter idref="freqParameter.secondhalf" name="log"/>
        <distribution idref="treeLikelihood.firsthalf" name="log"/>
        <distribution idref="treeLikelihood.secondhalf" name="log"/>
        <distribution idref="CoalescentConstant.firsthalf" name="log"/>
        <parameter idref="popSize.firsthalf" name="log"/>
    </logger>

    <logger id="screenlog" logEvery="100000" mode="autodetect">
        <distribution idref="posterior" name="log"/>
        <!--   TODO    -->
        <log arg="@posterior" id="ESS.0" spec="beast.base.inference.util.ESS"/>
        <distribution idref="likelihood" name="log"/>
        <distribution idref="prior" name="log"/>
    </logger>

    <tree estimate="false" id="RandomTree.firsthalf" initial="@Tree.firsthalf" name="init" populationModel="@ConstantPopulation.firsthalf" spec="beast.base.spec.evolution.tree.coalescent.RandomTree" taxa="@firsthalf"/>

</run>

</beast>
